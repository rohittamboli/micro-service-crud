create database bank;

use bank;




CREATE TABLE ACCOUNT_HOLDER(ACCOUNT_HOLDER_ID INT(10) PRIMARY KEY AUTO_INCREMENT,FIRST_NAME VARCHAR(20) NOT NULL,LAST_NAME VARCHAR(20) NOT NULL,EMAIL VARCHAR(50),ADDRESS VARCHAR(500) NOT NULL);

CREATE TABLE ACCOUNT(ACCOUNT_ID INT(10) PRIMARY KEY AUTO_INCREMENT,ACOUNT_NUMBER INT(10),BALANCE INT,ACCOUNT_HOLDER_ID INT(10),CONSTRAINT FW_KEY FOREIGN KEY(ACCOUNT_HOLDER_ID) REFERENCES ACCOUNT_HOLDER(ACCOUNT_HOLDER_ID));


